{
  "name": "gatsby-typescript-tailwind",
  "private": false,
  "description": "A gatsby starter kit using TypeScript and Tailwind CSS",
  "version": "0.1.247",
  "author": "Florentin Eckl <ecklf@icloud.com>",
  "dependencies": {
    "babel-preset-gatsby": "^0.4.6",
    "core-js": "^3.6.5",
    "framer-motion": "^1.11.0",
    "gatsby-plugin-netlify": "^2.3.3",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-zeit-now": "^0.3.0",
    "react": "16.13.1",
    "react-clipboard.js": "^2.0.16",
    "react-div-100vh": "^0.3.8",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0"
  },
  "devDependencies": {
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.0.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "gatsby": "2.22.4",
    "gatsby-image": "2.4.5",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-manifest": "2.4.8",
    "gatsby-plugin-offline": "3.2.6",
    "gatsby-plugin-postcss": "2.3.2",
    "gatsby-plugin-react-helmet": "3.3.2",
    "gatsby-plugin-sharp": "2.6.8",
    "gatsby-plugin-typescript": "2.4.3",
    "gatsby-source-filesystem": "2.3.7",
    "gatsby-transformer-sharp": "2.5.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "tailwindcss": "1.4.6",
    "typescript": "3.9.3"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "tailwind css",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "clean": "yarn clean",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/impulse/gatsby-typescript-tailwind"
  },
  "bugs": {
    "url": "https://github.com/impulse/gatsby-typescript-tailwind/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
